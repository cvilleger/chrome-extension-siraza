{
  "name": "Sir-Aza Chrome Extension",
  "version": "0.1.0",
  "description": "Sir-Aza Chrome Extension!",
  "permissions": ["notifications"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Sir-Aza Chrome Extension",
    "default_icon": "images/icon.png"
  },
  "web_accessible_resources": [
    "images/icon.png",
    "images/icon-green.png",
    "images/icon-red.png"
  ],
  "icons": {
    "128": "images/icon.png"
  },
  "manifest_version": 2
}
